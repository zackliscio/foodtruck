<!DOCTYPE html>
<html>
<head>
  <title>Foodtruck</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
 
<div class="navbar">
  <div class="navbar-inner">
    <div class="container">
      <a class="brand" href= "<%=root_path%>" >
 Trucky Awards
</a>
    </div>
  </div>
</div>

<div class = "container">
	<% if flash[:notice] %>
      <p class="notice"><%= flash[:notice] %></p>
    <% end %>
    <% if flash[:error] %>
      <p class="error"><%= flash[:error] %></p>
    <% end %>
<%= yield %>
</div>
</body>
</html>
